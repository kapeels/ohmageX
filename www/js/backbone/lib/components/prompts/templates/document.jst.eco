<form>
  <h3 class="hide-accessible"><%= @displayLabel %></h3>
  <div class="file-control">
    <label for="<%= @id %>" class="markdown-inset-block"><%- @promptText %></label>
    <div class="container">
      <h3 class="file-header"><%= @fileName %></h3>
      <div class="input-activate">
        <button class="get-file" type="button">Get File</button>
        <input name="<%= @id %>" type="file" />
      </div>
    </div>
  </div>
</form>
